{
	"collectCoverage": true,
	"collectCoverageFrom": [
		"src/**/*.{js,jsx}"
	],
	"coveragePathIgnorePatterns": [
		"/ext-lib/"
	],
	"coverageThreshold": {
		"global": {
			"branches": 90,
			"lines": 90
		},
		"<rootDir>/**/*.js": {
			"branches": 90,
			"lines": 90
		}
	},
	"notify": true,
	"notifyMode": "always",
	"clearMocks": true,
	"rootDir": "../../../"
}