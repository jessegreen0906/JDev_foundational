{
	"collectCoverage": true,
	"collectCoverageFrom": [
		"src/**/*.{js,jsx}"
	],
	"coveragePathIgnorePatterns": [
		"/ext-lib/"
	],
	"coverageThreshold": {
		"../../../**/*.js": {
			"branches": 90,
			"lines": 90
		}
	},
	"notify": false,
	"notifyMode": "always",
	"clearMocks": true,
	"rootDir": "../../../",
	"testMatch": [
		"**/test/unit-tests/**/?(*.)test.js?(x)"
	],
	"testURL": "http://localhost"
}